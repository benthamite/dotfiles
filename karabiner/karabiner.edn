{:des "Emacs: right_command + spacebar ‚Üí A-C-"
:rules [
        [[:right_command :spacebar] :A-C- :emacs]
        ]}

{:des "Emacs: right_command + left_control ‚Üí A-M-"
:rules [
        [[:right_command :left_control] :A-M- :emacs]
